//
#include<stdio.h>
int main()
{
	int a,n,d,sum,sum1;
	printf("enter a number:");
	scanf("%d",&n);
	printf("enter a number to divide:");
	scanf("%d",&d);
	a=1;
	sum=0,sum1=0;
	while(a<=n)
	{
		if(a%d==0)
		{
		 printf("%d ",a);
		 if(a%2==0)
		   sum=sum+a;
		 else if(a%2!=0)
		     sum1=sum1+a;
	     }
	    a++;
	}
	printf("\nsum of even=%d",sum);
	printf("\nsum of odd=%d",sum1);
	return 0;
}
